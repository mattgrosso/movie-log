{
  "name": "cinema-roll",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "update-version": "node src/assets/javascript/version.js",
    "serve": "nodemon --watch .env --exec \"vue-cli-service serve\"",
    "build": "yarn update-version && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "deploy": "yarn build && aws s3 sync dist/ s3://cinemaroll --profile home && aws cloudfront create-invalidation --distribution-id ETF4I2E64GQSC --paths '/*' --profile home > invalidation_output.txt"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/multimonth": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@fullcalendar/vue3": "^6.1.15",
    "@popperjs/core": "^2.11.8",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^7.36.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.7",
    "dotenv": "^16.4.7",
    "firebase": "^9.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "ordinal-js": "^1.1.0",
    "randomcolor": "^0.6.2",
    "register-service-worker": "^1.7.2",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.13",
    "vue-chart-3": "^3.1.8",
    "vue-router": "^4.5.0",
    "vue3-calendar-heatmap": "^2.0.5",
    "vue3-click-away": "^1.2.4",
    "vue3-lazyload": "^0.3.8",
    "vuewordcloud": "^19.0.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.9",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "vitest": "1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:promise/recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/no-unused-components": "off",
      "vue/multi-word-component-names": "off",
      "no-trailing-spaces": "warn",
      "eol-last": "off",
      "quotes": [
        "off",
        "double"
      ],
      "semi": "off",
      "comma-dangle": "off",
      "space-before-function-paren": "warn",
      "indent": "warn",
      "no-undef": "warn",
      "padded-blocks": "warn",
      "object-curly-spacing": "warn",
      "array-bracket-spacing": "warn",
      "no-unused-vars": "warn",
      "no-empty": "warn",
      "prefer-const": "warn",
      "no-multiple-empty-lines": "warn",
      "no-useless-return": "off",
      "no-debugger": "warn",
      "vue/no-deprecated-slot-scope-attribute": "warn",
      "space-infix-ops": "warn",
      "space-before-blocks": "warn",
      "no-unreachable": "warn",
      "no-constant-condition": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
